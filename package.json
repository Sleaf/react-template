    "prettier": "^2.0.4",
    "react-hot-loader": "^4.12.20",
    "stylelint": "^13.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "svg-sprite-loader": "^4.2.2",
    "terser-webpack-plugin": "^2.3.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-deep-scope-plugin": "^1.6.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT"
}
